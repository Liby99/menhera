main {
    let sum : (int, int) => int = (x : int, y : int) : int => x + y in
    let pi : float = 3.1415926 in
    let f : ([string]) => int = (args : [string]) : int =>
        let mi1 : maybe<int> = int::parse(args[0]),
            mi2 : maybe<int> = int::parse(args[1])
        in match (mi1) {
            Some(i1) => match (mi2) {
                Some(i2) => sum(i1, i2),
                None => -1
            },
            None => -1
        }
    in f
}
