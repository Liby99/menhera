let
    linked_list = () => [],
    push = (ls, e) => [e, ls],
    pop = (ls) => ls[1],
    peek = (ls) => ls[0],
    length = (ls) => if ls == [] then 0 else 1 + length(ls[1]),
    get = (ls, i) => if i == 0 then ls[0] else get(ls[1], i - 1),
    to_string = (ls) =>
        let f = (ls) =>
            if ls == [] then ""
            else if ls[1] == [] then ls[0]
            else ls[0] + ", " + to_string(ls[1])
        in "[" + f(ls) + "]"
in
    linked_list()
