namespace Point {
    new = (x, y, z) => (x, y, z),
    x = (p) => p[0],
    y = (p) => p[1],
    z = (p) => p[2],
    add = (p1, p2) => (p1[0] + p2[0], p1[1] + p2[1], p1[2] + p2[2]),
    subtract = (p1, p2) => (p1[0] - p2[0], p1[1] - p2[1]),
    dot = (p1, p2) => p1[0] * p2[0] + p1[1] * p2[1] + p1[2] * p2[2],
    cross = (p1, p2) => (p1[1] * p2[2] - p1[2] * p2[1], p1[2] * p2[0] - p1[0] * p2[2], p1[0] * p2[1] - p1[1] * p2[0]),
    to_string = (p) => "(" + p[0] + ", " + p[1] + ", " + ")"
}
